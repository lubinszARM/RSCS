action act_cmp_random_qlen_pair() {
    min(sq_md.min_random_qlen12, sq_md.qlen_random1, sq_md.qlen_random2);
    min(sq_md.min_random_qlen34, sq_md.qlen_random3, sq_md.qlen_random4);
}

table cmp_random_qlen_pair {
    actions {act_cmp_random_qlen_pair;}
    default_action : act_cmp_random_qlen_pair();
}

action act_cmp_random_qlen_all() {
    min(sq_md.min_random_qlen_all, sq_md.min_random_qlen12, sq_md.min_random_qlen34);
}

table cmp_random_qlen_all {
    actions {act_cmp_random_qlen_all;}
    default_action : act_cmp_random_qlen_all();
}

